{
   "version": "0.2.0",
   "configurations": [
      {
         "type": "node",
         "request": "launch",
         "name": "Start Project",
         "runtimeExecutable": "npm",
         "runtimeArgs": [
            "run-script",
            "start"
         ],
         "sourceMaps": true,
         "smartStep": true,
         "internalConsoleOptions": "openOnSessionStart",
         "outFiles": [
             "${workspaceFolder}/dist/**/*.js"
         ],
         "skipFiles": [
            "<node_internals>/**"
         ],
         "sourceMapPathOverrides": {
            "webpack:///src/*": "${workspaceFolder}/src/*"
         },
         "resolveSourceMapLocations": [
            "${workspaceFolder}/**",
            "!**/node_modules/**"
         ]
      }
   ]
}
